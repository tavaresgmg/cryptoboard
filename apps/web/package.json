{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" \"vite.config.ts\"",
    "typecheck": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:real": "E2E_REAL_BACKEND=true E2E_BASE_URL=http://127.0.0.1:8080 playwright test e2e/real-backend.spec.ts"
  },
  "dependencies": {
    "@crypto/shared": "workspace:*",
    "@vueuse/core": "^14.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.575.0",
    "reka-ui": "^2.8.2",
    "tailwind-merge": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.9",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.1"
  }
}
